{
    "manifest_version": 3,
    "name": "GetRolled",
    "version": "0.1.1",
    "description": "Spice up your browsing with surprise Rickrolls and add a dash of mischief to your browsing routine!",
    "permissions": [
      "tabs",
      "webNavigation",
      "storage",
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/GetRolled16.png",
    "48": "icons/GetRolled48.png",
    "128": "icons/GetRolled128.png"
  }
}